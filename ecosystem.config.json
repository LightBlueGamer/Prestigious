{
  "apps": [
    {
      "name": "Prestigious Bot",
      "script": "./start.sh",
      "interpreter": "bash",
      "instances": 1,
      "autorestart": true,
      "watch": true,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "exec_mode": "fork",
      "max_memory_restart": "2G"
    }
  ]
}
