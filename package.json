{
  "name": "prestigious",
  "version": "1.0.0-13",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node --es-module-specifier-resolution=node dist/index.js",
    "deploy": "npm run build && node --es-module-specifier-resolution=node dist/bot/deploy.js",
    "compile": "tsc --build --verbose",
    "prettier": "prettier src --write",
    "build": "npm run prettier && npm run compile && npx typedoc --options typedoc.json",
    "docs": "npm run build && npx http-server docs/"
  },
  "author": "Tim",
  "license": "ISC",
  "description": "Prestigious is an RPG bot designed to be a hard grind, good luck!",
  "type": "module",
  "dependencies": {
    "@joshdb/core": "^1.2.7",
    "@joshdb/mongo": "^1.1.6",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "pretty-ms": "^9.0.0"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.1.6",
    "@sapphire/ts-config": "^4.0.1",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "^4.2.3"
  }
}
